{
	"name": "prodrate",
	"version": "0.0.6",
	"scripts": {
		"dev": "vite dev --https --host",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/card": "^6.0.0-beta.16",
		"@smui/common": "^6.0.0-beta.16",
		"@smui/fab": "^6.0.0-beta.16",
		"@smui/icon-button": "^6.0.0-beta.16",
		"@smui/list": "^6.0.0-beta.16",
		"@smui/menu": "^6.0.0-beta.16",
		"@smui/paper": "^6.0.0-beta.16",
		"@smui/snackbar": "^6.0.0-beta.16",
		"@smui/top-app-bar": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.20.6",
		"@types/web": "^0.0.69",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@typescript/lib-dom": "npm:@types/web",
		"@vitejs/plugin-basic-ssl": "^1.2.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^6.2.7",
		"vitest": "^3.0.9"
	},
	"type": "module",
	"dependencies": {
		"@undecaf/barcode-detector-polyfill": "^0.9.14",
		"idb-keyval": "^6.2.0"
	}
}