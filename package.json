{
	"name": "prodrate",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev --https --host",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview --https --host",
		"prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@playwright/test": "^1.22.2",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/common": "^6.0.0-beta.16",
		"@smui/fab": "^6.0.0-beta.16",
		"@smui/icon-button": "^6.0.0-beta.16",
		"@smui/paper": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.49.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2"
	},
	"type": "module",
	"dependencies": {
		"@zxing/library": "^0.19.1",
		"idb-keyval": "^6.2.0"
	}
}
